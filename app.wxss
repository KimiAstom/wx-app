@import "styles/iconfont.wxss";

page,
view,
text,
swiper,
swiper-item,
image,
navigator {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

page {
    --themeColor: #eb4450;
    font-size: 28rpx;
}

.clearfix:after {
    content: "";
    display: block;
    height: 0px;
    clear: both;
    visibility: hidden;
}